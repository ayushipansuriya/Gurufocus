{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "view.py": {
      "runtime": "python3.11",
      "excludeFiles": "{tests/**,__tests__/**,**/*.test.py,**/test_*.py,fixtures/**,__fixtures__/**,static/**,assets/**,media/**,node_modules/**,__pycache__/**}"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "view.py"
    }
  ]
}
